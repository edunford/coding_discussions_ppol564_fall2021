    "#Set Up\n",
    import csv
    "with open('us-states.csv') as file:\n",
    "    state_covid_data = []\n",
    "    for row in csv.reader(file):\n",
    "        state_covid_data.append(row)\n",
    "\n",
    "len(state_covid_data)\n",
    "print(len(state_covid_data))\n",
    "\n",
    "state_covid_data[:5]"
   ]
